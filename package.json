{
  "name": "ricochet-js",
  "version": "0.1.5",
  "description": "Multi-purpose deploy once prototyping backend",
  "main": "dist/index.js",
  "bin": {
    "ricochet": "dist/cli.es5.js",
    "ricochet-js": "dist/cli.es5.js"
  },
  "scripts": {
    "dev": "nodemon  --exec babel-node src/cli.js | pino-tiny",
    "start": "pm2 start 'babel-node src/cli.js > server.log' --name ricochet-server",
    "stop": "pm2 stop ricochet-server",
    "test": "jest --watch",
    "test:server": "npx serve src/__test__/test.files",
    "coverage": "jest --coverage",
    "generateKeys": "babel-node src/generateKey.js",
    "build": "babel src -d dist --ignore 'src/**/__test__/'",
    "shebang": "echo '#!/usr/bin/env node' > dist/cli.es5.js && cat dist/cli.js >> dist/cli.es5.js",
    "prepare": "npm run build && npm run shebang",
    "watch": "npm run prepare && babel src -d dist --ignore 'src/**/__test__/' --watch",
    "i18n:parser": "npx i18next-parser 'src/**/*.js'"
  },
  "keywords": [
    "server",
    "json",
    "store",
    "backend",
    "prototyping"
  ],
  "author": "Jérémie Pardou",
  "repository": "https://github.com/jrmi/ricochetjs",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.799.0",
    "body-parser": "^1.19.0",
    "client2client.io": "^1.3.1",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "easy-no-password": "^1.2.0",
    "express": "^4.17.1",
    "express-request-language": "^1.1.15",
    "i18next": "^19.8.4",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.1.0",
    "mime-types": "^2.1.27",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nanoid": "^3.1.18",
    "nedb": "^1.8.0",
    "nodemailer": "^6.4.16",
    "pino": "^6.7.0",
    "pino-http": "^5.3.0",
    "socket.io": "^2.3.0"
  },
  "peerDependencies": {
    "webpack-sources": "~2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "babel-jest": "^26.6.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "pino-tiny": "^1.0.0",
    "pm2": "^4.5.0",
    "prettier": "^2.2.1",
    "supertest": "^4.0.2",
    "tempy": "^0.7.1"
  }
}
